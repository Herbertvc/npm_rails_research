{
  "name": "npm_research",
  "version": "0.0.0",
  "private": true,
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },  
  "scripts": {
    "start": "node ./bin/www",
    "build-js": "browserify app.js -o app/build.js",
    "watch-js": "watchify app.js -o build.js --poll --debug --verbose"    
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "watchify": "^3.7.0"
  }
}
